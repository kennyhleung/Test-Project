@model HealthpeuticsAPI.Models.SummaryViewModel

<div class="col-md-12">
    <div class="table=responsive">
     <table class="table">
      <tbody>
          <tr>
              <th>Patient Name</th>
              <td>@Model.Name</td>
          </tr>
          <tr>
              <th>Gender</th>
              <td>@Model.Gender</td>
          </tr>
          <tr>
              <th>DOB</th>
              <td>@Model.BirthDate</td>
          </tr>
          <tr>
              <th>Contact</th>
              <td>@Model.Phone</td>
          </tr>
          <tr>
              <th>Address</th>
              <td>@Model.Address</td>
          </tr>
      </tbody>        
     </table>   
    </div>   
</div>
